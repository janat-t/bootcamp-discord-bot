{
	"env": {
		"node": true
	},
	"extends": [
		"airbnb-base",
		"prettier",
		"plugin:node/recommended"
	],
	"plugins": [
		"prettier"
	],
	"rules": {
		"prettier/prettier": "error",
		"no-unused-vars": "warn",
		"no-restricted-syntax": "off",
		"no-underscore-dangle": "off",
		"node/no-unpublished-require": "off"
	}
}